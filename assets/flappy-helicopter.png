<svg width="200" height="200" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="skyBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E0F6FF;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="helicopter" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2ecc71;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#27ae60;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="pipe" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2c3e50;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#34495e;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="200" height="200" fill="url(#skyBg)" rx="15"/>
  
  <!-- Clouds -->
  <ellipse cx="50" cy="30" rx="20" ry="12" fill="rgba(255,255,255,0.8)"/>
  <ellipse cx="65" cy="30" rx="15" ry="8" fill="rgba(255,255,255,0.8)"/>
  <ellipse cx="150" cy="40" rx="25" ry="15" fill="rgba(255,255,255,0.8)"/>
  <ellipse cx="170" cy="40" rx="20" ry="10" fill="rgba(255,255,255,0.8)"/>
  
  <!-- Pipes -->
  <!-- Left pipe (top) -->
  <rect x="20" y="0" width="25" height="80" fill="url(#pipe)" rx="2"/>
  <rect x="15" y="70" width="35" height="15" fill="url(#pipe)" rx="3"/>
  
  <!-- Left pipe (bottom) -->
  <rect x="20" y="130" width="25" height="70" fill="url(#pipe)" rx="2"/>
  <rect x="15" y="125" width="35" height="15" fill="url(#pipe)" rx="3"/>
  
  <!-- Right pipe (top) -->
  <rect x="155" y="0" width="25" height="60" fill="url(#pipe)" rx="2"/>
  <rect x="150" y="50" width="35" height="15" fill="url(#pipe)" rx="3"/>
  
  <!-- Right pipe (bottom) -->
  <rect x="155" y="120" width="25" height="80" fill="url(#pipe)" rx="2"/>
  <rect x="150" y="115" width="35" height="15" fill="url(#pipe)" rx="3"/>
  
  <!-- Helicopter body -->
  <ellipse cx="90" cy="100" rx="20" ry="12" fill="url(#helicopter)"/>
  
  <!-- Helicopter cockpit -->
  <ellipse cx="88" cy="98" rx="12" ry="8" fill="#3498db" opacity="0.8"/>
  
  <!-- Helicopter tail -->
  <rect x="105" y="97" width="15" height="6" fill="url(#helicopter)" rx="3"/>
  
  <!-- Main rotor (animated) -->
  <line x1="65" y1="85" x2="115" y2="85" stroke="#34495e" stroke-width="3">
    <animateTransform attributeName="transform" type="rotate" 
                      values="0 90 85;360 90 85" dur="0.1s" repeatCount="indefinite"/>
  </line>
  
  <!-- Tail rotor (animated) -->
  <g transform="translate(120,95)">
    <line x1="-5" y1="0" x2="5" y2="0" stroke="#34495e" stroke-width="2">
      <animateTransform attributeName="transform" type="rotate" 
                        values="0;360" dur="0.05s" repeatCount="indefinite"/>
    </line>
  </g>
  
  <!-- Thrust particles -->
  <circle cx="75" cy="105" r="2" fill="#ff6b6b" opacity="0.7">
    <animate attributeName="cx" values="75;65;55" dur="0.3s" repeatCount="indefinite"/>
    <animate attributeName="opacity" values="0.7;0.3;0" dur="0.3s" repeatCount="indefinite"/>
  </circle>
  <circle cx="72" cy="108" r="1.5" fill="#ff6b6b" opacity="0.5">
    <animate attributeName="cx" values="72;62;52" dur="0.4s" repeatCount="indefinite"/>
    <animate attributeName="opacity" values="0.5;0.2;0" dur="0.4s" repeatCount="indefinite"/>
  </circle>
  <circle cx="78" cy="103" r="1" fill="#ff6b6b" opacity="0.6">
    <animate attributeName="cx" values="78;68;58" dur="0.25s" repeatCount="indefinite"/>
    <animate attributeName="opacity" values="0.6;0.3;0" dur="0.25s" repeatCount="indefinite"/>
  </circle>
  
  <!-- Score -->
  <rect x="10" y="10" width="40" height="20" fill="rgba(0,0,0,0.5)" rx="5"/>
  <text x="30" y="24" text-anchor="middle" fill="white" font-size="12" font-weight="bold">12</text>
  
  <!-- Motion lines -->
  <line x1="65" y1="100" x2="55" y2="100" stroke="rgba(255,255,255,0.5)" stroke-width="2"/>
  <line x1="65" y1="105" x2="50" y2="105" stroke="rgba(255,255,255,0.3)" stroke-width="1"/>
  
  <!-- Warning indicator for pipes -->
  <circle cx="32" cy="95" r="8" fill="rgba(255, 107, 107, 0.3)" stroke="#ff6b6b" stroke-width="2"/>
  <text x="32" y="100" text-anchor="middle" fill="#ff6b6b" font-size="12" font-weight="bold">!</text>
  
  <!-- Title -->
  <text x="100" y="190" text-anchor="middle" fill="#ffffff" font-size="11" font-weight="bold">FLAPPY HELICOPTER</text>
</svg>
<svg width="200" height="200" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="skyBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#98FB98;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="obj1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ff6b6b;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ee5a5a;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="obj2" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4ecdc4;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#44a08d;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="obj3" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#feca57;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ff9ff3;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="200" height="200" fill="url(#skyBg)" rx="15"/>
  
  <!-- Clouds -->
  <ellipse cx="40" cy="30" rx="15" ry="8" fill="rgba(255,255,255,0.7)"/>
  <ellipse cx="50" cy="30" rx="12" ry="6" fill="rgba(255,255,255,0.7)"/>
  <ellipse cx="160" cy="25" rx="18" ry="10" fill="rgba(255,255,255,0.7)"/>
  <ellipse cx="170" cy="25" rx="15" ry="8" fill="rgba(255,255,255,0.7)"/>
  
  <!-- Falling objects with emojis -->
  <rect x="30" y="60" width="20" height="20" fill="url(#obj1)" rx="3" transform="rotate(15 40 70)"/>
  <text x="40" y="75" text-anchor="middle" font-size="14">🍎</text>
  
  <rect x="80" y="40" width="18" height="18" fill="url(#obj2)" rx="3" transform="rotate(-10 89 49)"/>
  <text x="89" y="54" text-anchor="middle" font-size="12">🌟</text>
  
  <rect x="130" y="70" width="22" height="22" fill="url(#obj3)" rx="3" transform="rotate(25 141 81)"/>
  <text x="141" y="86" text-anchor="middle" font-size="16">🎈</text>
  
  <rect x="60" y="100" width="16" height="16" fill="url(#obj1)" rx="3" transform="rotate(-20 68 108)"/>
  <text x="68" y="112" text-anchor="middle" font-size="11">🍭</text>
  
  <rect x="150" y="45" width="19" height="19" fill="url(#obj2)" rx="3" transform="rotate(30 159 54)"/>
  <text x="159" y="59" text-anchor="middle" font-size="13">🎯</text>
  
  <rect x="40" y="120" width="17" height="17" fill="url(#obj3)" rx="3" transform="rotate(-15 48 128)"/>
  <text x="48" y="132" text-anchor="middle" font-size="12">💎</text>
  
  <!-- Motion lines -->
  <line x1="42" y1="50" x2="38" y2="58" stroke="rgba(255,255,255,0.5)" stroke-width="2"/>
  <line x1="91" y1="30" x2="87" y2="38" stroke="rgba(255,255,255,0.5)" stroke-width="2"/>
  <line x1="143" y1="60" x2="139" y2="68" stroke="rgba(255,255,255,0.5)" stroke-width="2"/>
  
  <!-- Answer box -->
  <rect x="70" y="140" width="60" height="25" fill="rgba(255,255,255,0.9)" rx="12" stroke="#333" stroke-width="2"/>
  <text x="100" y="157" text-anchor="middle" fill="#333" font-size="14" font-weight="bold">?</text>
  
  <!-- Title -->
  <text x="100" y="190" text-anchor="middle" fill="#ffffff" font-size="12" font-weight="bold">COUNTING</text>
</svg>
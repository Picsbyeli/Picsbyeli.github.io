<svg width="200" height="200" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="gradient1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4a90e2;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#7b68ee;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="gradient2" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ff6b6b;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ffa726;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="200" height="200" fill="url(#gradient1)" rx="20"/>
  
  <!-- Tower blocks -->
  <rect x="70" y="160" width="60" height="20" fill="url(#gradient2)" rx="2"/>
  <rect x="75" y="140" width="50" height="20" fill="#4ecdc4" rx="2"/>
  <rect x="80" y="120" width="40" height="20" fill="#ffe066" rx="2"/>
  <rect x="85" y="100" width="30" height="20" fill="#ff6b9d" rx="2"/>
  <rect x="90" y="80" width="20" height="20" fill="#c44569" rx="2"/>
  
  <!-- Falling block -->
  <rect x="95" y="40" width="10" height="10" fill="#ffd93d" rx="1" opacity="0.8"/>
  
  <!-- Balance beam -->
  <line x1="50" y1="180" x2="150" y2="180" stroke="#8e8e93" stroke-width="3"/>
  
  <!-- Text -->
  <text x="100" y="25" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="white" text-anchor="middle">TOWER</text>
  <text x="100" y="195" font-family="Arial, sans-serif" font-size="12" fill="white" text-anchor="middle" opacity="0.8">Balance</text>
</svg>
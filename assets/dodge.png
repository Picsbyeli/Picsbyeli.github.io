<svg width="200" height="200" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="spaceBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e3c72;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2a5298;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="player" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4ecdc4;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#44a08d;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="enemy" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ff6b6b;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ee5a5a;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="coin" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ffd700;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#ffed4e;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="200" height="200" fill="url(#spaceBg)" rx="15"/>
  
  <!-- Stars -->
  <circle cx="30" cy="40" r="1" fill="rgba(255,255,255,0.8)"/>
  <circle cx="60" cy="25" r="1.5" fill="rgba(255,255,255,0.9)"/>
  <circle cx="90" cy="35" r="1" fill="rgba(255,255,255,0.7)"/>
  <circle cx="130" cy="20" r="1" fill="rgba(255,255,255,0.8)"/>
  <circle cx="170" cy="45" r="1.5" fill="rgba(255,255,255,0.9)"/>
  <circle cx="40" cy="80" r="1" fill="rgba(255,255,255,0.6)"/>
  <circle cx="150" cy="85" r="1" fill="rgba(255,255,255,0.8)"/>
  <circle cx="20" cy="120" r="1.5" fill="rgba(255,255,255,0.7)"/>
  <circle cx="180" cy="110" r="1" fill="rgba(255,255,255,0.9)"/>
  
  <!-- Falling enemy blocks -->
  <rect x="50" y="30" width="25" height="25" fill="url(#enemy)" rx="3" transform="rotate(15 62 42)"/>
  <rect x="120" y="50" width="30" height="20" fill="url(#enemy)" rx="3" transform="rotate(-10 135 60)"/>
  <rect x="30" y="90" width="20" height="28" fill="url(#enemy)" rx="3" transform="rotate(25 40 104)"/>
  <rect x="160" y="80" width="22" height="22" fill="url(#enemy)" rx="3" transform="rotate(-15 171 91)"/>
  
  <!-- Motion lines for falling blocks -->
  <line x1="65" y1="20" x2="60" y2="28" stroke="rgba(255,100,100,0.5)" stroke-width="2"/>
  <line x1="138" y1="40" x2="133" y2="48" stroke="rgba(255,100,100,0.5)" stroke-width="2"/>
  <line x1="43" y1="80" x2="38" y2="88" stroke="rgba(255,100,100,0.5)" stroke-width="2"/>
  
  <!-- Coins -->
  <circle cx="80" cy="70" r="8" fill="url(#coin)" stroke="#f39c12" stroke-width="2"/>
  <text x="80" y="75" text-anchor="middle" fill="#f39c12" font-size="10" font-weight="bold">$</text>
  
  <circle cx="140" cy="130" r="8" fill="url(#coin)" stroke="#f39c12" stroke-width="2"/>
  <text x="140" y="135" text-anchor="middle" fill="#f39c12" font-size="10" font-weight="bold">$</text>
  
  <!-- Player character at bottom -->
  <rect x="85" y="150" width="30" height="30" fill="url(#player)" rx="4"/>
  <!-- Player face -->
  <circle cx="93" cy="158" r="3" fill="white"/>
  <circle cx="107" cy="158" r="3" fill="white"/>
  <rect x="95" y="168" width="10" height="3" fill="white" rx="1"/>
  
  <!-- Movement arrow -->
  <path d="M 70 165 L 80 165 M 75 160 L 80 165 L 75 170" stroke="#4ecdc4" stroke-width="3" fill="none"/>
  
  <!-- Score display -->
  <rect x="10" y="10" width="50" height="20" fill="rgba(0,0,0,0.5)" rx="5"/>
  <text x="35" y="24" text-anchor="middle" fill="white" font-size="12" font-weight="bold">1250</text>
  
  <!-- Title -->
  <text x="100" y="190" text-anchor="middle" fill="#ffffff" font-size="14" font-weight="bold">DODGE</text>
</svg>